{
  "name": "rngpa-ts",
  "version": "0.0.1",
  "description": "React Native Google Places Typescript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "images"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "tsc",
    "prepare": "husky || true",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "release": "npx release-it"
  },
  "author": "loiclienard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loiclienard/rngpa-ts.git"
  },
  "keywords": [
    "autocomplete",
    "google",
    "places",
    "react-component",
    "react-native",
    "ios",
    "android",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/loiclienard/rngpa-ts/issues"
  },
  "homepage": "https://github.com/loiclienard/rngpa-ts#readme",
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "qs": "~6.9.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash.debounce": "^4.0.9",
    "@types/qs": "^6.9.16",
    "@types/react": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-native": "^0.76.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.47.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": ">= 0.60"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
